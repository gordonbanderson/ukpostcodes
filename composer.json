{
    "name": "suilven/ukpostcodes",
    "description": "Wrapper around postcodes.io to get info about UK postcodes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gordon Anderson",
            "email": "gordon.b.anderson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php-vcr/php-vcr": "^1.4",
        "php-vcr/phpunit-testlistener-vcr": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0@dev",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpunit/phpunit": "^7.5",
        "clue/graph-composer": "dev-master"
    },

    "autoload": {
        "psr-4": {
            "Suilven\\UKPostCodes\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Suilven\\UKPostCodes\\": "tests"
        }
    }
}
